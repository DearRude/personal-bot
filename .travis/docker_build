#!/bin/bash

echo $DOCKERHUB_TOKEN | docker login -u $DOCKERHUB_USER --password-stdin
echo $CR_PAT | docker login ghcr.io -u $DOCKERHUB_USER --password-stdin
docker build . -t osm-detective
docker images

docker tag osm-detective $DOCKERHUB_USER/personal-bot:$TAG
docker push $DOCKERHUB_USER/personal-bot:$TAG
docker tag osm-detective ghcr.io/$DOCKERHUB_USER/personal-bot:$TAG
docker push ghcr.io/$DOCKERHUB_USER/personal-bot:$TAG
